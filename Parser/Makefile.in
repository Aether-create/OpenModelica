CC=@CC@
CXX=@CXX@
CFLAGS=@CFLAGS@ -Wall -DANTLR3_NODEBUGGER @EXTRA_CFLAGS_GENERATED_CODE@
CPPFLAGS=@CPPFLAGS@ -I$(COMPILERHOME) $(RMLINC) -I. -I$(ANTLR) -I$(ANTLR)/include -I../3rdParty/gc/include $(BUILDINC)
