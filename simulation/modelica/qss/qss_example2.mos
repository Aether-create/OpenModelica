// name: qss_example2
// status: correct

loadString("
within ;
package qssTests
  model example2
    Real x1, x2;
  initial equation
    x1 = 0;
    x2 = 20;
  equation
    der(x1) = 0.01*x2;
    der(x2) = -100*x1 - 100*x2 + 2020;
  end example2;
end qssTests;
"); getErrorString();

simulate(qssTests.example2, startTime=0.0, stopTime=500.0, method="qss"); getErrorString();

