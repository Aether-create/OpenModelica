          <GraphCell style="Graph" closed="false">
            <Input>{val(rectangleArea,0), val(triangleArea,0),
val(circlePerimeter,0),val(circleArea,0)}</Input>
            <Output>{20.0,10.0,21.9911485752,38.4845100066}</Output>
