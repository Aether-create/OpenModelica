          </TextCell>
          <GraphCell style="Graph" closed="false">
            <Input>function findName
  input String nameArray[:];
  input String name;
  output Integer index;
protected
  Integer i, length = size(nameArray, 1);
algorithm
  index := -1;
  i := 1;
  while index == -1 and i&lt;length loop
    if nameArray[i] == name then
      index := i;
    end if;
    i := i + 1;
  end while;
  assert(index&lt;>-1, "findName: Failed");
end findName;</Input>
            <Output>{findName}</Output>
          </GraphCell>
          <GraphCell style="Graph" closed="false">
            <Input>findName({"C++", "Lisp", "Java", "Ada", "Modelica"},"Lisp")</Input>
            <Output>2</Output>
          </GraphCell>
