all:
	$(MAKE) omc @OMLIBRARY_TARGET@
	$(MAKE) omplot

.PRECIOUS: Makefile

omc:
	$(MAKE) -C OMCompiler
omlibrary-core:
	$(MAKE) -C libraries core
omlibrary-all:
	$(MAKE) -C libraries all
omplot:
	$(MAKE) -C OMPlot

clean:
	test ! -d build || rm -r build/

Makefile: Makefile.in config.status
	./config.status
config.status: configure
	./config.status -recheck
configure: configure.ac
	autoconf
