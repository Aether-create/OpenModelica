        txt = Tpl.writeTok(txt, Tpl.ST_LINE("</original>\n"));
        txt = fun_82(txt, i_dae);
        txt = Tpl.softNewLine(txt);
        txt = Tpl.popBlock(txt);
        txt = Tpl.writeTok(txt, Tpl.ST_STRING("</flattening>"));
