          </TextCell>
          <GraphCell style="Graph" closed="false">
            <Input>class LightSwitchExtended
  Boolean on;
algorithm
  when time &lt; 5 or time >= 25 then
    on := false;
  elsewhen sample(5, 1) then
    if rem(time, 5) == 0 then
      on := true;
    else
      on := false;
    end if;
  end when;
end LightSwitchExtended;</Input>
            <Output>{LightSwitchExtended}</Output>
